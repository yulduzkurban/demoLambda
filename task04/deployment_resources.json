{
  "lambda-basic-execution": {
    "policy_content": {
      "Statement": [
        {
          "Action": [
            "logs:CreateLogGroup",
            "logs:CreateLogStream",
            "logs:PutLogEvents",
            "dynamodb:GetItem",
            "dynamodb:Query",
            "dynamodb:PutItem",
            "dynamodb:Batch*",
            "dynamodb:DeleteItem",
            "ssm:PutParameter",
            "ssm:GetParameter",
            "kms:Decrypt",
            "sqs:*",
            "sns:*"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "resource_type": "iam_policy",
    "tags": {}
  },
  "sqs_handler-role": {
    "predefined_policies": [],
    "principal_service": "lambda",
    "custom_policies": [
      "lambda-basic-execution"
    ],
    "resource_type": "iam_role",
    "tags": {}
  },
  "async_queue": {
    "resource_type": "sqs_queue",
    "fifo_queue": false,
    "region": "eu-central-1",
    "visibility_timeout": 30,
    "delay_seconds": 0,
    "maximum_message_size": 1024,
    "message_retention_period": 60,
    "receive_message_wait_time_seconds": 20,
    "policy": {},
    "redrive_policy": {
      "deadLetterTargetArn": "arn:aws:sqs:eu-central-1:859465876068:dead-letter-queue",
      "maxReceiveCount": 1
    },
    "content_based_deduplication": false,
    "tags": {},
    "dependencies": [
      {
        "resource_name": "sqs_handler-role",
        "resource_type": "iam_role"
      }
    ]
  },
  "sns_handler-role": {
    "predefined_policies": [],
    "principal_service": "lambda",
    "custom_policies": [
      "lambda-basic-execution"
    ],
    "resource_type": "iam_role",
    "tags": {}
  },
  "lambda_topic": {
    "resource_type": "sns_topic",
    "region": "eu-central-1",
    "event_sources": [],
    "tags": {},
    "dependencies": [
      {
        "resource_name": "sns_handler-role",
        "resource_type": "iam_role"
      }
    ]
  }
}